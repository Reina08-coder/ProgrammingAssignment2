library(MASS)
makeCacheMatrix <- function (x =matriz()){
inv <- NULL 
set <- function (y) {
x <<- y
inv <<- NULL
}
get <- function () {x}
setinverse <- function (inverse)inv <<- inverse
getinverse <- function () {
iver <- ginv(x)
inver%*%x
}
lis(set=set, get=get, setinverse=setinverse, getInverse = getInverse)
}
cachesolve <- function (x, ...){
inv <- x$getinv ()
if (!is.null(inv))){
message(“getting cached data!”)
return(inv)
}
data <- x$get()
inv <- solve(data, …)
X$setinv(inv)
Inv
}

